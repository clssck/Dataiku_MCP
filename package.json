{
  "name": "dataiku-mcp",
  "version": "1.1.0",
  "mcpName": "io.github.clssck/dataiku-mcp",
  "description": "MCP server for Dataiku DSS REST API",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "bin/dataiku-mcp-server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/clssck/Dataiku_MCP"
  },
  "homepage": "https://github.com/clssck/Dataiku_MCP",
  "bugs": {
    "url": "https://github.com/clssck/Dataiku_MCP/issues"
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "start": "npx tsx src/index.ts",
    "build": "tsc",
    "check": "tsc --noEmit",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "biome:check": "biome check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run --project integration",
    "check:metadata": "node scripts/check-server-json-sync.mjs",
    "prepublishOnly": "npm run check && npm run check:metadata && npm test && npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.15",
    "@types/node": "^25.2.3",
    "tsx": "^4.19.0",
    "typescript": "^5.8.0",
    "vitest": "^4.0.18"
  }
}
